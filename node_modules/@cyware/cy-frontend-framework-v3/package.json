{
  "name": "@cyware/cy-frontend-framework-v3",
  "version": "3.7.14",
  "description": "Cyware Frontend Framework",
  "type": "module",
  "files": [
    "css/",
    "es/",
    "lib/"
  ],
  "main": "lib/index.cjs",
  "module": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./*": {
      "import": "./es/*",
      "require": "./lib/*"
    },
    "./css/*": {
      "import": "./css/*",
      "require": "./css/*"
    },
    "./resolver": {
      "import": "./es/utilities/resolver.js",
      "require": "./lib/utilities/resolver.cjs"
    },
    "./theme": {
      "import": "./es/utilities/theme.js",
      "require": "./lib/utilities/theme.cjs"
    },
    "./timezones": {
      "import": "./es/utilities/timezones.js",
      "require": "./lib/utilities/timezones.cjs"
    },
    "./apiService": {
      "import": "./es/utilities/api-service.js",
      "require": "./lib/utilities/api-service.cjs"
    },
    "./mfa": {
      "import": "./es/utilities/mfa.js",
      "require": "./lib/utilities/mfa.cjs"
    },
    "./index.css": "./css/index.css",
    "./style-guide.css": "./css/style-guide.css"
  },
  "style": "./css/index.css",
  "scripts": {
    "clear": "rimraf css && rimraf es && rimraf lib",
    "dev": "vite",
    "build:vue": "vue-tsc && vite build",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier .  --write",
    "build:style": "rimraf css && gulp --require @esbuild-kit/cjs-loader -f ./internal/build/build-scss.ts",
    "build": "npm run clear && gulp --require @esbuild-kit/cjs-loader -f ./internal/build/gulpfile.ts",
    "preview": "vite preview",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish:lib": "bash scripts/publish.sh"
  },
  "publishConfig": {
    "ignore": [
      "src/",
      "node_modules/"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/language": "^6.10.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@cyware/common-design-system": "^1.2.3",
    "@cyware/cyware-icons": "^1.0.13",
    "@popperjs/core": "^2.11.7",
    "@storybook/addon-a11y": "^8.2.9",
    "axios": "^1.8.3",
    "buffer": "^6.0.3",
    "element-plus": "^2.3.2",
    "froala-editor": "^4.4.0",
    "highlight.js": "^10.7.1",
    "js-cookie": "^2.2.1",
    "luxon": "^3.3.0",
    "marked": "^13.0.2",
    "mixpanel-browser": "^2.45.0",
    "placeholder-loading": "^0.6.0",
    "radash": "^11.0.0",
    "sanitize-html": "^2.12.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.0",
    "v3-infinite-loading": "^1.3.1",
    "vue-codemirror": "^6.1.1",
    "vue-component-meta": "^2.0.26",
    "vue-froala-wysiwyg": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@chromatic-com/storybook": "^1.6.1",
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@rollup/plugin-image": "^3.0.2",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@types/sanitize-html": "^2.9.0",
    "@types/sortablejs": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "babel-loader": "^8.3.0",
    "chalk": "^5.3.0",
    "clean-css-cli": "^5.6.2",
    "consola": "^3.2.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.21.2",
    "glob": "^10.3.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "npmignore": "^0.3.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "sass": "^1.60.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.0",
    "vue": "^3.2.45",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.24"
  },
  "author": "cyware",
  "license": "ISC"
}
